<h5>Projects</h5>
<%= link_to "New", new_project_path(@project), class: "btn btn-success" %> 
<hr>
<div class="row">
    <% @projects.each do |project| %>
        <div class="col-md-4">
        <%= link_to project_path(project) do %>
            <div class="card">
                <div class="card-body">
                    <%= project.name %>
                </div>
            </div>
        <% end %>
        </div>
    <% end %>

